<h2 id="ej2" title="Ejemplo listas">Ejemplo listas</h2>
	<!--Lista ordenada-->
	<h3>Lista ordenada</h3>
	<ol>
		<li>elemento de la lista 1</li>
		<li>elemento de la lista 2</li>
		<li>elemento de la lista 3</li>
		<li>elemento de la lista 4</li>
	</ol>
	
	<!--Lista sin orden-->
	<h3>Lista sin orden</h3>
	<ul>
		<li>elemento de lista 1</li>
		<li>elemento de lista 2</li>
		<li>elemento de lista 3</li>
		<li>elemento de lista 4</li>
	</ul>
